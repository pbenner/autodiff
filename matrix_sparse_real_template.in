#define VECTOR_ITERATOR         STR_CONCAT(VECTOR_NAME, Iterator)
#define MATRIX_JOINT_ITERATOR   STR_CONCAT(MATRIX_NAME, JointIterator)
#define MATRIX_JOINT3_ITERATOR  STR_CONCAT(MATRIX_NAME, Joint3Iterator)

